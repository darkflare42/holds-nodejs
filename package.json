{
  "name": "holds-nodejs",
  "version": "1.0.0",
  "description": "Holds POC in Nodejs",
  "main": "sever.js",
  "scripts": {
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "jest --forceExit",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./app/server.ts",
    "prod": "tsc && node ./build/server.js",
    "babel-node": "babel-node --presets=env",
    "start": "nodemon --exec babel-node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeConnect/holds-nodejs.git"
  },
  "author": "Or Keren",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WeConnect/holds-nodejs/issues"
  },
  "homepage": "https://github.com/WeConnect/holds-nodejs#readme",
  "dependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.2",
    "@types/validator": "^10.11.1",
    "amqplib": "^0.5.3",
    "apollo-server": "^2.6.9",
    "apollo-server-express": "^2.6.9",
    "bluebird": "^3.5.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "graphql": "^14.4.2",
    "graphql-relay": "^0.6.0",
    "graphql-sequelize": "^9.3.6",
    "le_node": "^1.8.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "rabbit.js": "^0.4.4",
    "rollbar": "^2.8.1",
    "sequelize": "^5.10.1",
    "sequelize-cli": "^5.5.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3",
    "umzug": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.4.4",
    "apollo-server-testing": "^2.7.0",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1"
